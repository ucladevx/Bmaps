{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "698514710897.dkr.ecr.us-east-2.amazonaws.com/mappening_backend:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [{
          "hostPort": 5000,
          "containerPort": 5000
      }]
    },
    {
      "name": "frontend",
      "image": "698514710897.dkr.ecr.us-east-2.amazonaws.com/mappening_frontend:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [{
          "hostPort": 80,
          "containerPort": 80
      }],
      "links": [
          "backend"
      ]
    }
  ]
}
